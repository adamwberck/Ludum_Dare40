    xPdir *=-1;
    yPdir *=-1;
}



if(yPdir&gt;0){
    if (vy &lt; 0){
        vy = Approach(vy, 0, tempYSkid);   
    }else {
        vy = Approach(vy, tempVyMax, tempYAccel);
    }
}
else if(yPdir&lt;0)
{
    if (vy &gt; 0){
        vy = Approach(vy, 0, tempYSkid);   
