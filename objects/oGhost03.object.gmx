tempSkid  = airSkid;
//YMovement
tempYAccel = YAccel;
tempYFric  = YFric;
tempYSkid  = YSkid;



tempVxMax = vxMax+random_range(-vxMax/10,+vxMax/10);
tempVyMax = vyMax+random_range(-vyMax/10,+vyMax/10);

// Ghost Float

var yPdir = oPlayer.y-y;

if(abs(yPdir)&lt;3){
    //yPdir = 0;
}

var xPdir = oPlayer.x-x;

if(abs(xPdir)&lt;3){
    xPdir = 0;
}



if(damaged){//slow
    tempVxMax = vxMax*2;
    tempVyMax = vyMax*2;
    sprite_index = sGhostHurt;
    flee = true;
    alarm[1]=60;
}
else{
    sprite_index = sGhost;    
}
damaged = false;

