    }
    else {
        vy = 0;
        break;
    }
}



repeat(abs(vxNew)) {
    if (!place_meeting(x+ sign(vxNew),y,oParSolid)) {
        with (oParEntity) {
            if (place_meeting(x - sign(vxNew), y, other.id))
                x += sign(vxNew);
                
