    var difB=(oPlayerStats.bombs /22);
    var difA=(oPlayerStats.arrows/100);
    var difI=(ds_list_size(oPlayerStats.inventory)/3);
    var dif = mean(difC,difB,difA,difI);
    
    draw_set_color(c_red);
    draw_set_font(font2);
    draw_set_halign(fa_left);
    var sDif = string_format(round(100*dif),3,0);
    draw_text(guiWidth-130,16,"Difficulty");
    draw_text(guiWidth-100,36,sDif+"%");
    
    
    var i=0;
    for(i=0;i&lt;hp;i++){
        //draw_sprite(sHeart,image_index,30+20*i,20);
        var xShift = 20;
        draw_sprite_ext(sHeart,image_index,35+22*i,20,1.2,
            1.2,0,c_white,1);
    }
    for(i=i;i&lt;maxhp;i++)
    {
        var xShift = 20;
        draw_sprite_ext(sHeartWhite,image_index,35+22*i,20,1.2,
            1.2,0,c_white,1);
    }
